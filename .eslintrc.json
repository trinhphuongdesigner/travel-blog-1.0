{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "globals": {
    "Atomics": "readonly",
    "ShareArrayBuffer": "readonly"
  },
  "plugins": ["prettier"],
  "rules": {
    "consistent-return": "off",
    "linebreak-style": 0
  }
}