{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "globals": {
    "Atomics": "readonly",
    "ShareArrayBuffer": "readonly"
  },
  "plugins": ["prettier"],
  "rules": {
    "consistent-return": "off",
    "linebreak-style": 0
    // "prettier/prettier": "error",
    // "no-unused-vars": "warn",
    // "no-console": "off",
    // "func-names": "off",
    // "no-process-exit": "off",
    // "object-shorthand": "off",
    // "class-methods-use-this": "off"
  }
}